 */

.page-header {
    background-color: #eee;
}

.page-title {
    margin: 0 auto;
    max-width: 1040px;
    padding: 30px 0;
    width: 100%;
}

.page-content {
    margin: 0 auto;
    padding: 40px 0;
    width: 100%;
}

.sidebar .page-content {
    margin: 0 auto;
    max-width: 1040px;
    padding: 40px 376px 40px 15px;
}


/**
 * 5.11 404
