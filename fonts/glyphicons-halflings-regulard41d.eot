 */

.archive-header {
    background-color: #eee;
}

.archive-title,
.archive-meta {
    margin: 0 auto;
    padding: 0 0 30px;
    width: 100%;
}

.archive-meta {
    font-size: 16px;
    font-style: normal;
    font-weight: normal;
    margin-top: -15px;
    padding: 0 0 11px;
}

/**
 * 5.10 Search Results/No posts
